{
  "name": "zoov",
  "version": "0.3.1",
  "author": "InfiniteXyy",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/esm/index.mjs",
      "import": "./dist/esm/index.js",
      "default": "./dist/index.js"
    },
    "./effect": {
      "types": "./dist/effect.d.ts",
      "module": "./dist/esm/effect.mjs",
      "import": "./dist/esm/effect.js",
      "default": "./dist/effect.js"
    }
  },
  "homepage": "https://github.com/InfiniteXyy/zoov",
  "description": "A React modular state-management solution, based on Zustand",
  "keywords": [
    "React",
    "State-management",
    "Zustand",
    "Modular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/InfiniteXyy/zoov"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md",
    "effect.d.ts"
  ],
  "scripts": {
    "build": "rimraf ./dist && rollup -c rollup.config.ts",
    "prepare": "npm run build",
    "test": "tsd && vitest run --coverage",
    "test:types": "tsd",
    "example": "cd example && vite",
    "taze": "taze -w major"
  },
  "tsd": {
    "directory": "test"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@testing-library/react": "^13.2.0",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@vitejs/plugin-react": "^1.3.2",
    "c8": "^7.11.3",
    "immer": "^9.0.14",
    "jsdom": "^19.0.0",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-tracked": "^1.7.9",
    "rimraf": "^3.0.2",
    "rollup": "^2.74.1",
    "rxjs": "^7.5.5",
    "scheduler": "^0.22.0",
    "taze": "^0.6.1",
    "tsd": "^0.20.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.9",
    "zustand": "^4.0.0-rc.1"
  },
  "peerDependencies": {
    "immer": ">=7.0.0",
    "react": ">=16.8.0",
    "rxjs": ">=7.0.0",
    "zustand": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "rxjs": {
      "optional": true
    }
  }
}
