{
  "name": "zoov",
  "version": "0.5.0",
  "author": "InfiniteXyy",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/esm/index.mjs",
      "import": "./dist/esm/index.js",
      "default": "./dist/index.js"
    },
    "./effect": {
      "types": "./dist/effect.d.ts",
      "module": "./dist/esm/effect.mjs",
      "import": "./dist/esm/effect.js",
      "default": "./dist/effect.js"
    },
    "./tracked": {
      "types": "./dist/tracked.d.ts",
      "module": "./dist/esm/tracked.mjs",
      "import": "./dist/esm/tracked.js",
      "default": "./dist/tracked.js"
    }
  },
  "homepage": "https://github.com/InfiniteXyy/zoov",
  "description": "A React modular state-management solution, based on Zustand",
  "keywords": [
    "React",
    "State-management",
    "Zustand",
    "Modular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/InfiniteXyy/zoov"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md",
    "effect.d.ts",
    "tracked.d.ts"
  ],
  "scripts": {
    "build": "rimraf ./dist && rollup -c rollup.config.mjs",
    "prepare": "npm run build",
    "test": "npm run test:types && vitest run --coverage",
    "test:types": "tsc -p ./tsconfig.build.json && tsd",
    "example": "cd example && vite",
    "taze": "taze -w major"
  },
  "tsd": {
    "directory": "test"
  },
  "devDependencies": {
    "@redux-devtools/extension": "^3.2.5",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.27.2",
    "immer": "^9.0.18",
    "jsdom": "^21.0.0",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tracked": "^1.7.11",
    "redux": "^4.2.0",
    "rimraf": "^4.1.1",
    "rollup": "^3.10.1",
    "rxjs": "^7.8.0",
    "scheduler": "^0.23.0",
    "taze": "^0.8.5",
    "tsd": "^0.25.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.27.2",
    "zustand": "^4.3.2"
  },
  "dependencies": {
    "proxy-compare": "^2.4.0"
  },
  "peerDependencies": {
    "immer": ">=7.0.0",
    "react": ">=16.8.0",
    "react-tracked": ">=1.0.0",
    "rxjs": ">=7.0.0",
    "zustand": ">=3 || >= 4.0.0-rc"
  },
  "peerDependenciesMeta": {
    "rxjs": {
      "optional": true
    },
    "react-tracked": {
      "optional": true
    }
  }
}
