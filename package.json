{
  "name": "zoov",
  "version": "0.2.2",
  "author": "InfiniteXyy",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs",
      "types": "./dist/utils.d.ts"
    }
  },
  "homepage": "https://github.com/InfiniteXyy/zoov",
  "description": "A React modular state-management solution, based on Zustand",
  "keywords": [
    "React",
    "State-management",
    "Zustand",
    "Modular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/InfiniteXyy/zoov"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "tsd && jest --coverage",
    "test:types": "tsd",
    "example": "parcel serve ./example/index.html --dist-dir .cache",
    "taze": "taze -w major"
  },
  "peerDependencies": {
    "immer": ">=7.0.0",
    "react": ">=16.8.0",
    "zustand": ">=3.3.0"
  },
  "optionalDependencies": {
    "rxjs": ">=7.5.5"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "codecov": "^3.8.3",
    "immer": "^9.0.12",
    "jest": "^27.5.1",
    "parcel": "^2.3.2",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-tracked": "^1.7.7",
    "rxjs": "^7.5.5",
    "taze": "^0.4.6",
    "ts-jest": "^27.1.3",
    "tsd": "^0.19.1",
    "typescript": "^4.6.2",
    "unbuild": "^0.7.0",
    "zustand": "^3.7.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  },
  "tsd": {
    "directory": "test"
  }
}
