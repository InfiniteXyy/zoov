{
  "name": "zoov",
  "version": "1.0.0-alpha.0",
  "author": "InfiniteXyy",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs",
      "types": "./dist/utils.d.ts"
    }
  },
  "homepage": "https://github.com/InfiniteXyy/zoov",
  "description": "A React modular state-management solution, based on Zustand",
  "keywords": [
    "React",
    "State-management",
    "Zustand",
    "Modular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/InfiniteXyy/zoov"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "tsd && vitest run --coverage",
    "test:types": "tsd",
    "example": "cd example && vite",
    "taze": "taze -w major"
  },
  "peerDependencies": {
    "immer": ">=7.0.0",
    "react": ">=16.8.0",
    "zustand": ">=4.0.0"
  },
  "optionalDependencies": {
    "rxjs": ">=7.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.1.1",
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@vitejs/plugin-react": "^1.3.1",
    "c8": "^7.11.0",
    "immer": "^9.0.12",
    "jsdom": "^19.0.0",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-tracked": "^1.7.9",
    "rxjs": "^7.5.5",
    "taze": "^0.5.0",
    "tsd": "^0.20.0",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.4",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "zustand": "^4.0.0-rc.0"
  },
  "tsd": {
    "directory": "test"
  }
}
